{
  "name": "tm-store-express",
  "version": "1.0.0",
  "description": "tm store api express",
  "main": "server.js",
  "author": "Tuấn Hin",
  "license": "TM",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuanmjnh/tm-store-express.git"
  },
  "scripts": {
    "start": "set NODE_ENV=production && export NODE_ENV=production && node src/server.js",
    "dev": "set NODE_ENV=development && export NODE_ENV=development && nodemon src/server.js",
    "dev:win": "set NODE_ENV=development && nodemon src/server.js",
    "test": "echo \\\"Error: no test specified\\\" && exit 1",
    "build": "concurrently \"webpack --mode production\"",
    "build:prod": "concurrently \"cross-env webpack --mode production\"",
    "mongo:start": "net start MongoDB",
    "mongo:stop": "net stop MongoDB",
    "mongo:remove": "\"C:\\MongoDB\\mongod.exe\" — remove",
    "mongo:path": "sudo service mongod stop && sudo mongod --replSet rs0 --dbpath /media/tuanmjnh/data/database/MongoDB/data",
    "eslint:init": "eslint --init",
    "docker:delete-images": "docker image prune -a",
    "docker:images": "docker images",
    "docker:run": "docker run -d -p 8080:8100 gcr.io/tm-store-4576e/tm-store-api:1.0.0",
    "docker:running": "docker ps",
    "docker:remove": "docker rm -f 1809af6a68b9",
    "docker:build": "docker build --tag tm-store-api:1.0.0 .",
    "docker:build-image": "docker tag tm-store-api:1.0.0 gcr.io/tm-store-4576e/tm-store-api",
    "docker:content": "docker run -it gcr.io/tm-store-4576e/tm-store-api:1.0.0 sh",
    "docker:build-gcr": "docker build --tag gcr.io/tm-store-4576e/tm-store-api:1.0.0 .",
    "docker:gcloud-image": "docker push gcr.io/tm-store-4576e/tm-store-api:1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.7.0",
    "moment": "^2.29.2",
    "mongoose": "^6.2.9",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "errorhandler": "^1.5.1",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "node": {
    "console": "empty",
    "fs": "empty",
    "net": "empty",
    "tls": "empty"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  }
}
